local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/Xv3nm/TotemModestMenuLib/main/source')))()

local Window = library:CreateWindow("Spilter V2 -- Not Premium",false)

local Tab = Window:NewTab("Main",false)

Tab:NewButton("Infinite yield",function() loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))() end, false)

Tab:NewToggle("Godmode", "Grants The Player God Mode", function(v)
        getgenv().godmode = v
        while true do
            if not getgenv().godmode then return end
            print("Debug")
            game.Players.LocalPlayer.Character.Humanoid.Health = math.huge
            wait()
        end
    end)

Tab:NewValueSelection("Walkspeed",16,1000,5,50,function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Tab:NewValueSelection("Jumppower",16,1000,5,50,function(s)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
